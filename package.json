{
  "name": "Bookings_Service",
  "version": "",
  "description": "",
  "author": "",
  "license": "",
  "engines": {
    "node": ">=6.13.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/client/**/*.{js,jsx}"
    ],
    "coverageDirectory": "<rootDir>/__test__/coverage/",
    "moduleNameMapper": {
      "\\.(css|scss|svg)$": "<rootDir>/__test__/config/emptyModule.js"
    },
    "resolver": null,
    "setupTestFrameworkScriptFile": "<rootDir>/__test__/config/setup.js",
    "testMatch": [
      "**/__test__/*.js",
      "<rootDir>/client/**/*.test.{js,jsx}"
    ]
  },
  "scripts": {
    "build": "webpack -d",
    "start": "node server/database/dataGenerator.js && node server/index.js",
    "react-dev": "webpack -d --watch",
    "server-dev": "nodemon --inspect server/index.js",
    "seed-db": "node ./server/database/dataGenerator.js",
    "serve": "grunt s3",
    "test": "jest",
    "lint": "eslint client/**.jsx client/components/**.jsx"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "mongodb": "^3.1.1",
    "mongodb-core": "^3.1.0",
    "mongoose": "^5.2.4",
    "nodemon": "^1.18.3",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-preset-airbnb": "^2.5.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "grunt": "^1.0.3",
    "grunt-aws": "^0.6.2",
    "grunt-contrib-nodeunit": "^2.0.0",
    "grunt-contrib-uglify": "^3.4.0",
    "grunt-s3": "^0.2.0-alpha.3",
    "jest": "^23.4.1",
    "jest-cli": "^23.4.1",
    "jest-enzyme": "^6.0.2",
    "pre-commit": "^1.2.2",
    "puppeteer": "^1.6.1",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.0.8"
  }
}
